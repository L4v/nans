close
clear
%%
% 1.)
x = [0 1.25 2.5 3.75 5];
fX = [1.7499 0.9830 1.2554 3.0802 2.3664];
% a)
scatter(x, fX);
hold on;
% b)
p = lSquares(x, fX, 3)
xs = linspace(min(x), max(x), 100);
px = polyval(p, xs);
plot(xs, px, 'red');

%%
% 2.)
close
clear
x = [1 2 3 5 6];
fx = [2 4 4 1 3];
% a)
scatter(x, fx);
hold on;
% b)
p = lSquares(x, fX, 4);
xs = linspace(min(x), max(x), 100);
px = polyval(p)